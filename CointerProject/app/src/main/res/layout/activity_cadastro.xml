<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#2E5266"
    tools:context=".view.CadastroActivity">

    <ImageView
        android:id="@+id/userImage"
        android:layout_width="177dp"
        android:layout_height="179dp"
        app:srcCompat="@drawable/user"
        tools:layout_editor_absoluteX="142dp"
        tools:layout_editor_absoluteY="29dp" />

    <TextView
        android:id="@+id/textViewCadastre"
        android:layout_width="114dp"
        android:layout_height="28dp"
        android:text="Cadastre-se"
        android:textColor="#E2EFDE"
        android:textSize="20dp"
        tools:layout_editor_absoluteX="173dp"
        tools:layout_editor_absoluteY="208dp" />

    <Button
        android:id="@+id/botaoCadastrar"
        android:layout_width="265dp"
        android:layout_height="50dp"
        android:background="#F6AE2D"
        android:text="Cadastrar"
        android:textColor="#E2EFDE"
        android:textSize="18dp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="98dp"
        tools:layout_editor_absoluteY="571dp" />

    <EditText
        android:id="@+id/editTextNome"
        android:layout_width="268dp"
        android:layout_height="45dp"
        android:backgroundTint="#F6AE2D"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="nome"
        android:textColor="#B3BFB8"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="96dp"
        tools:layout_editor_absoluteY="272dp" />

    <EditText
        android:id="@+id/editTextDataNasc"
        android:layout_width="268dp"
        android:layout_height="45dp"
        android:backgroundTint="#F6AE2D"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="data de nascimento"
        android:textColor="#B3BFB8"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="96dp"
        tools:layout_editor_absoluteY="343dp" />

    <EditText
        android:id="@+id/editTextSenha"
        android:layout_width="268dp"
        android:layout_height="45dp"
        android:backgroundTint="#F6AE2D"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="senha"
        android:textColor="#B3BFB8"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="96dp"
        tools:layout_editor_absoluteY="483dp" />

    <EditText
        android:id="@+id/editTextEmail"
        android:layout_width="268dp"
        android:layout_height="45dp"
        android:backgroundTint="#F6AE2D"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="e-mail"
        android:textColor="#B3BFB8"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="96dp"
        tools:layout_editor_absoluteY="415dp" />

</androidx.constraintlayout.widget.ConstraintLayout>